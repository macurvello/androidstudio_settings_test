<keymap version="1" name="Default copy" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="CopyPaths" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorDuplicate" />
  <action id="MethodDown" />
  <action id="MethodUp" />
  <action id="MoveLineDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
    <keyboard-shortcut first-keystroke="alt down" />
  </action>
  <action id="MoveLineUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
    <keyboard-shortcut first-keystroke="alt up" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="shift alt r" />
  </action>
  <action id="RerunTests" />
</keymap>